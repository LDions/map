}

/**
 * 公司部门关联表
 * @author JeeSite
 */
entity SysCompanyOffice {
    sysCompanyId  	        String maxlength(100)            /** 公司ID */
    sysOfficeId             String maxlength(100)            /** 机构ID */
}

/**
 * 员工岗位
 * @author JeeSite
 */
entity SysPost {
    postCode                String maxlength(100)           /** 岗位编码 */
    postName	            String maxlength(100)           /** 岗位名称 */
    postType	            PostType 	                    /** 岗位分类（高管、中层、基层） */
    status                  PostStatusType	                /** 状态 */
    remarks                 String maxlength(500)           /** 备注信息 */
}

/**
 * 岗位分类
 * 高管|中层|基层
 * @author JeeSite
 */
enum PostType {
    SENIOR,
    MIDDLE,
    BASIC
}

/**
 * 机构状态类型
 * 正常|删除|停用
 * @author JeeSite
 */
enum PostStatusType {
    NORMAL,
    DELETE,
    DISABLE
}

/**
 * 员工与岗位关联表
 * @author JeeSite
 */
entity SysEmployeePost {
    sysEmployeeId  	        String maxlength(100)            /** 员工ID */
    sysPostId               String maxlength(100)            /** 岗位ID */
}

/**
 * 角色数据权限表
 * @author JeeSite
 */
entity SysRoleDataScope {
    sysRoleId  	            Long                            /** 控制角色ID */
    ctrlType                ControlType                     /** 控制类型 */
    ctrlData                String maxlength(64)            /** 控制数据，如控制类型为公司，那么这里就是公司编号 */
    ctrlPermi               String maxlength(64)            /** 控制权限 ??????????????????? */
}

/**
 * 控制类型
 * 公司级|机构级
 * @author JeeSite
 */
enum ControlType {
    COMPANY,
    OFFICE
}

/**
 * 用户数据权限表
 * @author JeeSite
 */
entity SysUserDataScope {
    sysUserId  	            String maxlength(100)           /** 控制用户ID */
    ctrlType                ControlType                     /** 控制类型 */
    ctrlData                String maxlength(64)            /** 控制数据，如控制类型为公司，那么这里就是公司编号 */
    ctrlPermi               String maxlength(64)            /** 控制权限 ???????????????????*/
}

/**
 * 接口表
 */
 entity SysApi {
    name            String           /** 接口名称 */
    requestMethod   String           /** 请求方法 */
    url             String           /** 路径 */
 }

 /**
  * 角色接口关联表
  */
  entity SysRoleApi {
     sysRoleId       Long           /** 角色外键 */
     sysApiId        Long           /** 接口外键 */
  }
